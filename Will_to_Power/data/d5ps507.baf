
IF
	Global("D5_PS507","GLOBAL",0)
THEN
	RESPONSE #100
		SetGlobal("D5_CONT","GLOBAL",1)
		SetNumTimesTalkedTo(1)
		ActionOverride(LastSummonerOf(Myself),StartDialogOverride("d5_ps507",Myself))
END

IF
	True()
THEN
	RESPONSE #100
		SetGlobal("D5_PS507","GLOBAL",0)
		SetNumTimesTalkedTo(0)
		DestroySelf()
END

